  "scripts": {
    "test": "jest",
    "test:watch": "jest --watchAll",
    "test:coverage": "jest --coverage",
    "lerna:init": "lerna init",
    "lerna:bootstrap": "lerna bootstrap",
    "lerna:clean": "lerna clean",
    "linter": "esw --ext .js --ext .jsx packages --watch --color --ignore-path .gitignore --cache",
    "dev": "npm-run-all --parallel serve:dev client",
    "client": "cross-env NODE_ENV=development webpack --progress --watch --config ./webpack.dev.js",
    "bundle:report": "cross-env APP_ENV=production NODE_ENV=production webpack --progress --config ./webpack.report.js",
    "build:clean": "rm -rf ./build/",
    "build:local": "yarn run build:clean && cross-env APP_ENV=local NODE_ENV=production webpack --config ./webpack.prod.local.js",
    "build:dev": "yarn run build:clean && cross-env APP_ENV=staging NODE_ENV=production webpack --config ./webpack.prod.js",
    "build:sw": "yarn run build:clean && cross-env APP_ENV=production NODE_ENV=production webpack --config ./webpack.prod.sw.js",
    "build": "yarn run build:clean && cross-env APP_ENV=production NODE_ENV=production webpack --progress --config ./webpack.prod.js",
    "serve:dev": "NODE_ENV=development nodemon ./packages/server/index.js",
    "serve": "NODE_ENV=production node ./packages/server/index.js",
    "mocking": "yarn json-server --watch ./packages/mocking/db.js"
  },
